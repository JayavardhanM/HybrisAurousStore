<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;my.aurous.facades.product.data.GenderData>"/>
	</bean>

	<bean class="my.aurous.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	
	<!-- MyEmployee Bean Class -->
	<bean class="my.aurous.facades.product.data.MyEmployeeData">
		<property name="empId" type="java.lang.String"/>
		<property name="empName" type="java.lang.String"/>
		<property name="empSalary" type="java.lang.Double"/>
		<property name="empWorkType" type="my.aurous.core.enums.WorkTypeEnumeration"/>
		<property name="empAnnualPay" type="java.lang.Double"/>
	</bean>
	
</beans>
